<div class="room5-bg mainContent">
  <div class="map" ui-sref='main.home'>
    <img src="/img/home/bedroomMap.png" alt="">
  </div>
  <div class="room-header">
    <label class="room-header-lab">餐厅</label>
    <label class="room-header-lab">Dining Room</label>
  </div>
  <ul class="room5-ul">
    <li ng-repeat="item in switchConfig" ng-class="{true:'open',false:''}[item.state]" ng-click="clickHandle(item)">
      <label class="room-switch-title">{{item.name}}</label>
    </li>
  </ul>

</div>
