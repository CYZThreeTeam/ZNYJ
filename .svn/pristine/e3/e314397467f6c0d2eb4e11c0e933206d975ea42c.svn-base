  /*margin-left: 50px;*/
}

.home-header{
  width: 100%;
  height:10%;
  background: url("../img/home/top_bg.png")no-repeat;
  background-size:100% 100%;
  box-sizing: border-box;
  display: -webkit-flex;
  position: absolute;
  /*left: 50px;*/
  top:0;
  /*opacity: 0.9;*/
  z-index: 4;
  padding:22px 0 5px 20px;
  -webkit-align-items: flex-end;
}

.home-content{
  /*padding-top:55px;*/
  width:100%;
  height:100%;
  position: absolute;
  left: 0;
  padding-left: 50px;
}

.home-bg{
  background: url("../img/home/home1.png")no-repeat;
  background-size:100% 100%;
  height:100%;
}

.home-footer{
  width:100%;
  height: 20%;
  /*background: url("../img/home/zy_11.png")no-repeat;*/
  background: RGBA(0,0,0,0.5);
  background-size:100% 100%;
  /*opacity:0.5;*/
  display: -webkit-flex;
  -webkit-justify-content: center;
  -webkit-align-items: center;
  position: absolute;
  left: 0;
  bottom:0;
  z-index: 4;
}
.home-footer li{
  width: 120px;height: 120px;
  display: inline-block;
  margin: 0 30px;color: #fff;text-align: center;line-height: 110px;font-size: 26px;;
}
.home-footer .footer-circle{
  position: absolute;
}
.home-footer .footer-circle li{
  border: 5px solid #ccc;border-radius: 50%;position: relative;transition: all 1s ease;
}
.home-footer.fadeOut{
  transition: all 1.5s ease;
  opacity: 0;visibility: hidden;
}
.home-footer.fadeOut li{
  transition: all 1s ease;
  transform: translateY(-8px);
  /*transition-duration*/
}
.home-footer.fadeOut li:nth-of-type(1){
  transition-duration:.8s;
}
.home-footer.fadeOut li:nth-of-type(2){
  transition-duration:.6s;
}
.home-footer.fadeOut li:nth-of-type(3){
  transition-duration:.4s;
}
.home-footer.fadeOut li:nth-of-type(4){
  transition-duration:.2s;
}
.home-footer li .l{
  position: absolute;
  left: -5px;top: -5px;width: 120px;height: 120px;
  border: 5px solid RGBA(67,236,200,0.5);border-radius: 50%;
  clip: rect(0px,60px,120px,0px);
  -webkit-transform: rotate(-80deg);
}
.home-footer li .l:after{
  content: ' ';
  width: 104px;height: 104px;position: absolute;
  left: 3px;top: 3px;border-radius: 50%;
  border: 1px solid #fff;
}
.home-footer li .r{
  position: absolute;
  left: -5px;top: -5px;width: 120px;height: 120px;
  border: 5px solid  RGBA(67,236,200,1);border-radius: 50%;
  clip: rect(0px,120px,120px,60px);
}
.home-footer li .r:after{
  content: ' ';
  width: 104px;height: 104px;position: absolute;
  left: 3px;top: 3px;border-radius: 50%;
  border: 1px solid #fff;
}

.home-footer .home-footer-active{
  -webkit-animation: rotate 3s linear infinite;
}

@-webkit-keyframes rotate{
  0%{-webkit-transform:rotate(0deg);}
  100%{-webkit-transform:rotate(360deg);}
}

