.top-icon{width: 254px;height: 36px;padding-left: 4px;padding-top:5px;}
.top-icon span{display:block; width: 40px; height: 25px; position:relative;float: left; margin-left:8px;background-image:url(../img/home/spirit.png);
}
.top-icon span i{display:block;width: 18px; height: 18px;background:red;color:white;border-radius: 50% 50%;position:absolute;font-style:normal;text-align: center;line-height: 18px;top: 0;right: -2px;
}
.top-icon span:nth-of-type(1){ background-position: 0 0px;}
.top-icon span:nth-of-type(1).on{ background-position: 0 -66px;}
.top-icon span:nth-of-type(2){background-position: -35px 0px;}
.top-icon span:nth-of-type(2).on{background-position: -35px -67px;}
.top-icon span:nth-of-type(3){background-position: -0px -31px;}
.top-icon span:nth-of-type(3).on{background-position: -0px -102px;}
.top-icon span:nth-of-type(4){background-position:-40px -30px ;}
.top-icon span:nth-of-type(4).on{background-position:-40px -102px ;}
.top-icon span:nth-of-type(5){background-position: -80px -31px;}
.top-icon span:nth-of-type(5).on{background-position: -80px -66px;}



